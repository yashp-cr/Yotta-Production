<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Tertiary_Partner_Source_Is_Needed</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
 ISPICKVAL(   TertiarySource__c , &apos;Alliance Partner&apos;),
 ISBLANK(    Partner_Tertiary__c )
)</errorConditionFormula>
    <errorDisplayField>Partner_Tertiary__c</errorDisplayField>
    <errorMessage>Tertiary Partner is required</errorMessage>
</ValidationRule>
