<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Support_To_LKUP_and_Name_Cannot_be_pop</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
 NOT(ISBLANK( Support_To__c )),
 NOT(ISBLANK( SupporttoCustomerName__c ))
)</errorConditionFormula>
    <errorMessage>Support To and Support To Customer Name cannot be populated together</errorMessage>
</ValidationRule>
