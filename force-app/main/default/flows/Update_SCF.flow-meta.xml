    <decisions>
        <name>Validate_SCF_Date</name>
        <label>Validate SCF Date</label>
        <locationX>1053</locationX>
        <locationY>555</locationY>
        <defaultConnector>
            <targetReference>Get_Subscriptions</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>SCF_Date_EndDate</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>SCF_Effective_Date</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <elementReference>ContractEndDate</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>SCF_Date_Error_Msg_1_Screen</targetReference>
            </connector>
            <label>SCF Date &gt; EndDate</label>
        </rules>
        <rules>
            <name>SCF_Date_Start_Date_Last_SCF_Date</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>SCF_Effective_Date</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <elementReference>LastSCFDate</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>SCF_Effective_Date</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <elementReference>ContractStartDate</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>SCF_Date_Error_Msg_2</targetReference>
            </connector>
            <label>SCF Date &lt; Start Date/Last SCF Date</label>
        </rules>
    </decisions>
