    <decisions>
        <name>Contract_Status_Check</name>
        <label>Contract Status Check</label>
        <locationX>810</locationX>
        <locationY>230</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Status_Approved_Activated_Expired</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>contractStatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Activated</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>contractStatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Approved</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>contractStatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Expired</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>SCF_Renewal_already_generated</targetReference>
            </connector>
            <label>Status = Approved/Activated/Expired</label>
        </rules>
        <rules>
            <name>Status_Not_Approved_Activated_Expired</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>contractStatus</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Activated</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>contractStatus</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Approved</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>contractStatus</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Expired</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Not_Active_Status_Error_Msg</targetReference>
            </connector>
            <label>Status != Approved/Activated/Expired</label>
        </rules>
    </decisions>
